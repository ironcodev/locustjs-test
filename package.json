  },
  "keywords": [
    "locustjs",
    "unit test",
    "unit testing",
    "tdd",
    "test",
    "test runner"
  ],
  "author": "Iron Code",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ironcodev/locustjs-test/issues"
  },
  "homepage": "https://github.com/ironcodev/locustjs-test#readme",
  "dependencies": {
    "@locustjs/base": "^4.0.0",
    "@locustjs/exception": "^2.0.2"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "rollup": "^4.29.1"
  }
}
